<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="tab_medium" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/notosans_medium</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontStyle">normal</item>
    </style>
    <style name="tab_semibold" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/notosans_semibold</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontStyle">normal</item>
    </style>


</resources>
